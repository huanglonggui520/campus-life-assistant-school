
page {
	flex: 1;
	width: 100%;
	height: 100%;
}
uni-button:after {
	border: none;
	border-radius: 0;
}
.content {

	display: flex;
	width: 750rpx;
	height: 100vh;

	flex-direction: column;
	flex: 1;
	background-color: #F9F9F9;
}
.bottom-back {
	margin-top: 10px;
	width: 750rpx;
	height: 44px;

	display: flex;

	flex-direction: column;
	justify-content: center;
	align-items: center;

	border: none;

	border-width: 0;
	border-radius: 0;
	background-color: #FFFFFF;
}
.bottom-back-text {
	font-size: 33rpx;
}
.mt10 {
	margin-top: 10px;
}
.content .uni-list {
	background-color: #F9F9F9;
}
.content .uni-list-item--disabled,.list-item {
	height: 50px;
	margin-bottom: 1px;
}



